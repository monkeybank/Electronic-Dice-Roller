    dice_roll_one = random(0,20);
    dice_roll_two = random(0,20);
    oled.drawBitmap(0, 0, epd_bitmap_d20_faces[dice_roll_one], 64, 64, WHITE);
    oled.drawBitmap(64, 0, epd_bitmap_d20_faces[dice_roll_two], 64, 64, WHITE);
    oled.display();
  }
  if (roll_mode == 2) {
    oled.clearDisplay();
    oled.invertDisplay(1);
    dice_roll_one = random(0,6);
    oled.fillRect(0, 0, 32, 64, WHITE);
    oled.drawBitmap(32, 0, epd_bitmap_d6_faces[dice_roll_one], 64, 64, WHITE);
    oled.fillRect(96, 0, 32, 64, WHITE);
    oled.display();
  }
  if (roll_mode == 3) {
    oled.clearDisplay();
    oled.invertDisplay(1);
    dice_roll_one = random(0,20);
    oled.fillRect(0, 0, 32, 64, WHITE);
    oled.drawBitmap(32, 0, epd_bitmap_d20_faces[dice_roll_one], 64, 64, WHITE);
    oled.fillRect(96, 0, 32, 64, WHITE);
    oled.display();
  }
  
  if (roll_mode == 0) {
    num_results[dice_roll_one + dice_roll_two]++;
    num_rolls++;
  }
}

void menuUpdate() {
  oled.clearDisplay();
  oled.invertDisplay(0);
  lineNum = 0;
  rowNum = 0;
  oled.setCursor(8*rowNum+24,8*lineNum);
  lineNum++;
  if (rowNum >= 16) {
    lineNum++;
    rowNum = 0;
  }
  if(lineNum >= 8) {
    lineNum = 0;
    rowNum = 0;
  }
  oled.println("Settings Menu");
  oled.setCursor(8*rowNum+24,8*lineNum);
  oled.println("Roll mode: ");
  rowNum += 8;
  oled.setCursor(8*rowNum+24,8*lineNum);
  oled.println(mode_names[roll_mode]);
  if (roll_mode == 0) {
    oled.drawBitmap(24, 24, epd_bitmap_mediumd66, 32, 32, WHITE);
    oled.drawBitmap(72, 24, epd_bitmap_mediumd66, 32, 32, WHITE);
  }
  else if (roll_mode == 1) {
    oled.drawBitmap(24, 24, epd_bitmap_mediumd2020, 32, 32, WHITE);
    oled.drawBitmap(72, 24, epd_bitmap_mediumd2020, 32, 32, WHITE);
  }
  else if (roll_mode == 2) {
    oled.drawBitmap(48, 24, epd_bitmap_mediumd66, 32, 32, WHITE);
  }
  else if (roll_mode == 3) {
    oled.drawBitmap(48, 24, epd_bitmap_mediumd2020, 32, 32, WHITE);
  }
  else if (roll_mode == 4) {
    int max_val = 0;
    for(int i=0;i<11;i++) {
      if(num_results[i] > max_val) {
        max_val = num_results[i];
      }
    }
    int hist_scale = max_val/max_size+1;
    oled.writeFillRect(5, 54-num_results[0]/hist_scale, 5, num_results[0]/hist_scale, WHITE);
    oled.writeFillRect(16, 54-num_results[1]/hist_scale, 5, num_results[1]/hist_scale, WHITE);
    oled.writeFillRect(27, 54-num_results[2]/hist_scale, 5, num_results[2]/hist_scale, WHITE);
    oled.writeFillRect(38, 54-num_results[3]/hist_scale, 5, num_results[3]/hist_scale, WHITE);
    oled.writeFillRect(49, 54-num_results[4]/hist_scale, 5, num_results[4]/hist_scale, WHITE);
    oled.writeFillRect(60, 54-num_results[5]/hist_scale, 5, num_results[5]/hist_scale, WHITE);
    oled.writeFillRect(71, 54-num_results[6]/hist_scale, 5, num_results[6]/hist_scale, WHITE);
    oled.writeFillRect(82, 54-num_results[7]/hist_scale, 5, num_results[7]/hist_scale, WHITE);
    oled.writeFillRect(93, 54-num_results[8]/hist_scale, 5, num_results[8]/hist_scale, WHITE);
    oled.writeFillRect(104, 54-num_results[9]/hist_scale, 5, num_results[9]/hist_scale, WHITE);
    oled.writeFillRect(115, 54-num_results[10]/hist_scale, 5, num_results[10]/hist_scale, WHITE);
    for(int i=0;i<11;i++) {
      if (i<8) {
        oled.setCursor(4+11*i, 56);
      }
      else {
        oled.setCursor(13*i-15, 56);
      }
      oled.println(i+2);
    }
  }
  else if (roll_mode == 5) {
    for(int i=0;i<11;i++) {
      if (i<=3) {
        oled.setCursor(0, i*10+24);
      }
      else if (i<=7) {
        oled.setCursor(42, (i-4)*10+24);
      }
      else {
        oled.setCursor(84, (i-8)*10+24);
      }
      String str_roll = String(i+2);
      String str_val = String(num_results[i]);
      oled.println(str_roll + ": " + str_val);
    }
    String str_total = String(num_rolls);
    oled.setCursor(84, 54);
    oled.println("All:" + str_total);
  }
  oled.display();
}

void setup() {
  pinMode(buttonOnePin, INPUT_PULLUP); // set arduino pin to input pull-up mode
  currentButtonOneState = digitalRead(buttonOnePin);

  const int random_one = analogRead(randomPin);
  delay(5);
  const int random_two = analogRead(randomPin);
  randomSeed(random_one<<10+random_two);

  //Serial.begin(9600);

  // initialize OLED display with address 0x3C for 128x64
  if (!oled.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    //Serial.println(F("SSD1306 allocation failed"));
    while (true);
  }

  oled.setTextSize(2);          // text size
  oled.setTextColor(WHITE);     // text color

  delay(2000); // wait for initializing
  oled.clearDisplay();
  oled.println("Dice");
  oled.println("Roller");
  oled.setTextSize(1);
  oled.println("Push the left");
  oled.println("button to roll");
  oled.println("Push the right button"); 
  oled.println("to change mode");
  oled.drawBitmap(80, 4, epd_bitmap_smalld66, 24, 24, WHITE);
  oled.drawRect(80, 4, 24, 24, WHITE);
  oled.drawBitmap(104, 4, epd_bitmap_smalld66, 24, 24, WHITE);
  oled.drawRect(104, 4, 24, 24, WHITE);
  oled.display();
  //oled.invertDisplay(1);
}

void loop() {
  lastButtonOneState = currentButtonOneState;      // save the last state
  currentButtonOneState = digitalRead(buttonOnePin); // read new state

  lastButtonTwoState = currentButtonTwoState;      // save the last state
  currentButtonTwoState = digitalRead(buttonTwoPin); // read new state

  if(lastButtonOneState == HIGH && currentButtonOneState == LOW) {
    rollDice();
  }

  if(lastButtonTwoState == HIGH && currentButtonTwoState == LOW) {
    menuUpdate();

    while(true) {
      lastButtonOneState = currentButtonOneState;      // save the last state
      currentButtonOneState = digitalRead(buttonOnePin); // read new state

      lastButtonTwoState = currentButtonTwoState;      // save the last state
      currentButtonTwoState = digitalRead(buttonTwoPin); // read new state

      if(lastButtonOneState == HIGH && currentButtonOneState == LOW) {
        rollDice();
        break;
      }
      if(lastButtonTwoState == HIGH && currentButtonTwoState == LOW) {
        roll_mode++;
        roll_mode %= num_modes;
        menuUpdate();
      }
    }
  }
}
